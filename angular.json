            "tsConfig": "projects/zebbra/ngx-neops-app-components/tsconfig.spec.json",
            "karmaConfig": "projects/zebbra/ngx-neops-app-components/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "neops-web-sdk"
}
